<?xml version="1.0"?>
<Problem>
	<Solvers>
		<ReactiveCompositionalMultiphaseOBL
			name="compflow"
			logLevel="1"
			discretization="fluidTPFA"
			targetRegions="{Region1}"
			enableEnergyBalance="0"
			maxCompFractionChange="1"
			numComponents="5"
            numSolidComponents="1"
			numPhases="2"
			transMultExp="4">
		<NonlinearSolverParameters
			lineSearchAction="None"
			timeStepDecreaseFactor="0.5"
			newtonTol="1.0e-4"
			newtonMaxIter="12"/>
		<LinearSolverParameters
			directParallel="0"/>
		</ReactiveCompositionalMultiphaseOBL>
	</Solvers>

	<Mesh>
		<InternalMesh
			name="mesh1"
			elementTypes="{C3D8}"
			xCoords="{0.0, 0.09}"
			yCoords="{0.0, 0.09}"
			zCoords="{0.0, 0.006}"
			nx="{100}"
			ny="{100}"
			nz="{1}"
			cellBlockNames="{block1}"/>
	</Mesh>

	<Geometry>
		<Box
			name="inj1"
			xMin="{-0.00001, -0.00001, -0.00001}"
			xMax="{0.00091, 0.09001, 0.00601}"/>
		<Box
			name="prd1"
			xMin="{0.08909, -0.00001, -0.00001}"
			xMax="{0.09001, 0.09001, 0.00601}"/>
	</Geometry>

	<Events
		maxTime="864000.0">
	</Events>

	<NumericalMethods>
		<FiniteVolume>
			<TwoPointFluxApproximation name="fluidTPFA"/>
		</FiniteVolume>
	</NumericalMethods>

	<ElementRegions>
		<CellElementRegion
			name="Region1"
			cellBlocks="{block1}"
			materialList="{rock, fluid}"/>
	</ElementRegions>

	<Constitutive>
		<CompressibleSolidConstantPermeability
			name="rock"
			solidModelName="nullSolid"
			porosityModelName="rockPorosity"
			permeabilityModelName="rockPerm"/>
		<NullModel
			name="nullSolid"/>
		<PressurePorosity
			name="rockPorosity"
			defaultReferencePorosity="1.0"
			referencePressure="10000000.0"
			compressibility="1e-20"/>
 		<ConstantPermeability
			name="rockPerm"
			permeabilityComponents="{1.233625e-11, 1.233625e-11, 1.233625e-11}"/>
		<OBLFluid
			name="fluid"
			interpolatorMode="adaptive"
			interpolatorType="multilinear"/>
	</Constitutive>

	<FieldSpecifications>
		<!-- initial conditions -->
		<FieldSpecification
			name="Region1InitialPressure"
			initialCondition="1"
			setNames="{ all }"
			objectPath="ElementRegions/Region1"
			fieldName="pressure"
			scale="10000000.0"/>
		<FieldSpecification
			name="Region1InitCalcite"
			initialCondition="1"
			setNames="{ all }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="0"
      		functionName="calciteFunc"
			scale="1.0"/>
		<FieldSpecification
			name="Region1InitCa"
			initialCondition="1"
			setNames="{ all }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="1"
			scale="0.0"/>
		<FieldSpecification
			name="Region1InitC"
			initialCondition="1"
			setNames="{ all }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="2"
			scale="0.0"/>
		<FieldSpecification
			name="Region1InitO"
			initialCondition="1"
			setNames="{ all }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="3"
			scale="0.33333333"/>
		<FieldSpecification
			name="Region1InitH"
			initialCondition="1"
			setNames="{ all }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="4"
			scale="0.66666666"/>
		<!-- injector -->
		<FieldSpecification
			name="inj1Pressure"
			objectPath="ElementRegions/Region1"
			fieldName="pressure"
			scale="10030000.0"
			setNames="{ inj1 }"/>
		<FieldSpecification
			name="inj1Calcite"
			setNames="{ inj1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="0"
			scale="1e-11"/>
		<FieldSpecification
			name="inj1Ca"
			setNames="{ inj1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="1"
			scale="1e-11"/>
		<FieldSpecification
			name="inj1C"
			setNames="{ inj1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="2"
			scale="4.73622828e-03"/>
		<FieldSpecification
			name="inj1O"
			setNames="{ inj1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="3"
			scale="0.33806956160195567"/>
		<FieldSpecification
			name="inj1H"
			setNames="{ inj1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="4"
			scale="0.6571942101127554"/>
		<!--SourceFlux
			name="sourceTerm1"
			objectPath="ElementRegions/Region1"
			scale="0.0"
			component="0"
			setNames="{ inj1 }"/>
		<SourceFlux
			name="sourceTerm2"
			objectPath="ElementRegions/Region1"
			scale="0.0"
			component="1"
			setNames="{ inj1 }"/>
		<SourceFlux
			name="sourceTerm3"
			objectPath="ElementRegions/Region1"
			scale="0.0"
			component="2"
			setNames="{ inj1 }"/>
		<SourceFlux
			name="sourceTerm4"
			objectPath="ElementRegions/Region1"
			scale="0.0"
			component="3"
			setNames="{ inj1 }"/>
		<SourceFlux
			name="sourceTerm5"
			objectPath="ElementRegions/Region1"
			scale="0.0"
			component="4"
			setNames="{ inj1 }"/-->
		<!-- producer -->
		<FieldSpecification
			name="prd1Pressure"
			objectPath="ElementRegions/Region1"
			fieldName="pressure"
			scale="10000000.0"
			setNames="{ prd1 }"/>
		<FieldSpecification
			name="prd1Calcite"
			setNames="{ prd1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="0"
			functionName="calciteFunc"
			scale="1.0"/>
		<FieldSpecification
			name="prd1Ca"
			setNames="{ prd1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="1"
			scale="0.0"/>
		<FieldSpecification
			name="prd1C"
			setNames="{ prd1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="2"
			scale="0.0"/>
		<FieldSpecification
			name="prd1O"
			setNames="{ prd1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="3"
			scale="0.33333333"/>
		<FieldSpecification
			name="prd1H"
			setNames="{ prd1 }"
			objectPath="ElementRegions/Region1"
			fieldName="globalCompFraction"
			component="4"
			scale="0.66666666"/>
	</FieldSpecifications>
	<Functions>
		<TableFunction 
			name="calciteFunc"
			inputVarNames="{elementCenter}"
			coordinateFiles="{xlin.geos, ylin.geos, zlin.geos}"
			voxelFile="calcite_2D.txt"
			interpolation="linear" />
	</Functions>
	<Outputs>
		<VTK
			name="vtkOutput"
			plotFileRoot="2d_setup_100"/>
	</Outputs>
</Problem>
